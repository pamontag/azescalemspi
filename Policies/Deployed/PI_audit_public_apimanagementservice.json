{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "microsoft.apimanagement/service"
      },
      {
        "not": {
          "count": {
            "field": "Microsoft.ApiManagement/service/publicIPAddresses[*]"
          },
          "greater": 0
        }
      }
    ]
  },
  "then": {
    "effect": "audit"
  }
}
